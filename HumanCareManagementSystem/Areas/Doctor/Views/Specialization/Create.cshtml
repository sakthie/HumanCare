@model Humancare.Data.Specialization

@{
    ViewBag.Title = "Create New Specialization";
    
}
@Html.Partial("DoctorNavigationPartial")
<h2>Create New Specialization</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Specialization</legend>

        <div class="editor-label">
            @Html.LabelFor(model => model.specialization)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.specialization)
            @Html.ValidationMessageFor(model => model.specialization)
        </div>

        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to Specialization List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
