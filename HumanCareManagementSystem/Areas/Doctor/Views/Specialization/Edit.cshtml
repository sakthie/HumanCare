@model Humancare.Data.Specialization

@{
    ViewBag.Title = "Edit Specialization";
}
@Html.Partial("DoctorNavigationPartial")
<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Specialization</legend>

        @Html.HiddenFor(model => model.id)

        <div class="editor-label">
            @Html.LabelFor(model => model.specialization)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.specialization)
            @Html.ValidationMessageFor(model => model.specialization)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to Specialisation List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
